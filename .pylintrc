[MASTER]
disable= missing-module-docstring, missing-class-docstring, missing-function-docstring, missing-final-newline
[FORMAT]
max-line-length=200

# allow 1 or 2 length variable names
good-names-rgxs=^[_a-zGW][_a-z0-9L]?$
good-names=D,T,D_row,S,A,F,H,F_vals, F_val, H_vals, Wilber, _Wilber, __Wilber, M
